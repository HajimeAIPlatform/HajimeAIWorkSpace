[
    {
        "sign_level": "中签",
        "sign_text": "千尺浮屠宝塔城，高峰顶上立停停，时人莫作寻常有，不是神仙难解登。",
        "sign_from": "文殊菩萨第一签"
    },
    {
        "sign_level": "下签",
        "sign_text": "满园桃李正开时，浅白深红色总宜，何以东风苦相妒，晓来折去最高枝。",
        "sign_from": "文殊菩萨第三签"
    },
    {
        "sign_level": "中签",
        "sign_text": "翩翩仙鹤欲凌云，整刷翎毛趁晓风，万里昂藏虽有志，争如人欲暗张弓。",
        "sign_from": "文殊菩萨第五签"
    },
    {
        "sign_level": "上签",
        "sign_text": "风云会虎问荣华，利路名扬信有涯，试问盘中争一掷，欢呼惊座满盘花。",
        "sign_from": "文殊菩萨第六签"
    },
    {
        "sign_level": "上签",
        "sign_text": "扁舟乘浪下沧波，万里浮云去作商，百货低昂从积蓄，倍收财宝富还乡。",
        "sign_from": "文殊菩萨第七签"
    },
    {
        "sign_level": "下签",
        "sign_text": "铅刀制体终无刃，石磬频敲岂有声，空里捉月兼捕影，谁知到底是虚名。",
        "sign_from": "文殊菩萨第十签"
    },
    {
        "sign_level": "下签",
        "sign_text": "两梗相争必有伤，争如忍欲胜相刚，若然闭口深藏舌，自有傍人说短长。",
        "sign_from": "文殊菩萨第十二签"
    },
    {
        "sign_level": "上签",
        "sign_text": "将军英誉果无双，谈笑平戎坐正邦，南北东西皆畏服，不劳兵马自归降。",
        "sign_from": "文殊菩萨第十三签"
    },
    {
        "sign_level": "上签",
        "sign_text": "春到园林万物生，蛰虫启户议求伸，人知南北东西去，得处应须遇贵人。",
        "sign_from": "文殊菩萨第十五签"
    },
    {
        "sign_level": "中签",
        "sign_text": "桑条无叶禾苗枯，春来将发物未苏，若要精严学正授，风霜一夜水平湖。",
        "sign_from": "文殊菩萨第十七签"
    },
    {
        "sign_level": "下签",
        "sign_text": "愁云恨雨作阴明，一醉花前唤不醒，若问贵人能安否，为谁游水已东倾。",
        "sign_from": "文殊菩萨第十八签"
    },
    {
        "sign_level": "上签",
        "sign_text": "天子端居正南面，两班文武缔玄谈，紫芝殿内祥云蔼，求嗣闺房得美男。",
        "sign_from": "文殊菩萨第十九签"
    },
    {
        "sign_level": "中签",
        "sign_text": "纷纷南北路歧多，觞景昏迷怎奈何，忽遇贵人相指引，此身方喜出尘罗。",
        "sign_from": "文殊菩萨第二十签"
    },
    {
        "sign_level": "上签",
        "sign_text": "客来问访子如何，田可耕兮地可锄，耕下一年成就处，不如秋末即冬初。",
        "sign_from": "文殊菩萨第二十一签"
    },
    {
        "sign_level": "上签",
        "sign_text": "喜袭眉间一点黄，诏书催子入朝堂，从今显达皆荣贵，自待天颜被宠光。",
        "sign_from": "文殊菩萨第二十二签"
    },
    {
        "sign_level": "中签",
        "sign_text": "围棋胜败合如何，到底还他一满棋，谁把真心劳苦力，不如及早去求和。",
        "sign_from": "文殊菩萨第二十三签"
    },
    {
        "sign_level": "下签",
        "sign_text": "父分子别妇离夫，箕豆相煎亲也疏，深夜分明月赖晓，谁知绕树有呜乌。",
        "sign_from": "文殊菩萨第二十四签"
    },
    {
        "sign_level": "下签",
        "sign_text": "平原兔死狗当烹，鸟尽弓藏事未亨，行满功成须退步，不然祸患及临身。",
        "sign_from": "文殊菩萨第二十七签"
    },
    {
        "sign_level": "中签",
        "sign_text": "人言日落是天涯，望断天涯未见家，千峰碧山相阻隔，碧山又被雾云遮。",
        "sign_from": "文殊菩萨第二十八签"
    },
    {
        "sign_level": "中签",
        "sign_text": "孽龙得雨亢峥嵘，位极名高成履冰，假若急流能勇退，免教满溢便高倾。",
        "sign_from": "文殊菩萨第二十九签"
    },
    {
        "sign_level": "上签",
        "sign_text": "择地安身渐有依，贵人指引在斯时，改过若遇龙蛇日，琴瑟和谐百事宜。",
        "sign_from": "文殊菩萨第三十签"
    },
    {
        "sign_level": "中签",
        "sign_text": "几年宦海苦淹财，欲进前程未有谐，一夜神人传好梦，紫坭封诏下天来。",
        "sign_from": "文殊菩萨第三十二签"
    },
    {
        "sign_level": "下签",
        "sign_text": "冯夷鼓浪涤为灾，断缆拖船去不回，纵有神人主暗佑，自身远害也损财。",
        "sign_from": "文殊菩萨第三十三签"
    },
    {
        "sign_level": "中签",
        "sign_text": "行藏对错要知机，老鹤何天不可飞，道合可行非则止，此身莫为稻梁肥。",
        "sign_from": "文殊菩萨第三十四签"
    },
    {
        "sign_level": "下签",
        "sign_text": "官府私门可怪哉，两人相见自疑猜，自春至夏无端谨，更合堤防笑里来。",
        "sign_from": "文殊菩萨第三十六签"
    },
    {
        "sign_level": "下签",
        "sign_text": "冤枉重重何得伸，犹如宝镜在埃尘，严霜积雪寒如许，争奈春来不遇春。",
        "sign_from": "文殊菩萨第三十七签"
    },
    {
        "sign_level": "上签",
        "sign_text": "扶桑拥出一红轮，照遍三千世界中，从此阴消阳道长，严霜积雪自销镕。",
        "sign_from": "文殊菩萨第三十九签"
    },
    {
        "sign_level": "上签",
        "sign_text": "归秋悠悠心浩然，远船鸣鼓过湾前，时来更备金风力，万里风帆一瞬间。",
        "sign_from": "文殊菩萨第四十签"
    },
    {
        "sign_level": "上签",
        "sign_text": "天上高高一枝丛，分明古断九秋风，乘时贵勇知无敌，殿试名扬第一功。",
        "sign_from": "文殊菩萨第四十一签"
    },
    {
        "sign_level": "下签",
        "sign_text": "玄武交临主破财，更防盗贼及非灾，就中有个知无敌，得自阴人天上来。",
        "sign_from": "文殊菩萨第四十二签"
    },
    {
        "sign_level": "下签",
        "sign_text": "勾陈主事事勾连，退步犹难进不前，若欲求官官未至，交争成讼病难痊。",
        "sign_from": "文殊菩萨第四十四签"
    },
    {
        "sign_level": "上签",
        "sign_text": "两意同谋必有期，宜从平地驾天梯，纤腰不许君求早，执手携笙舞袖低。",
        "sign_from": "文殊菩萨第四十五签"
    },
    {
        "sign_level": "下签",
        "sign_text": "白虎当门事不祥，能招横祸及灾殃，更防讼狱多忧制，不是官伤即血伤。",
        "sign_from": "文殊菩萨第四十六签"
    },
    {
        "sign_level": "下签",
        "sign_text": "天阴云暗雨悲风，终日昏迷似梦中，人去案头萤已死，鹤飞林下帐成空。",
        "sign_from": "文殊菩萨第四十七签"
    },
    {
        "sign_level": "中签",
        "sign_text": "昂藏野鹤在鸡群，玉石相逢且未分，倘借牙齿些子论，等闲平步上青云。",
        "sign_from": "文殊菩萨第四十八签"
    },
    {
        "sign_level": "上签",
        "sign_text": "否极应须有泰来，灵芝生出自超垓，阳回九十困春去，时至百花随分开。",
        "sign_from": "文殊菩萨第四十九签"
    },
    {
        "sign_level": "上签",
        "sign_text": "平原秋兔正当肥，雅胜将军脱猎归，谁把帛书得足雁，果然一箭中双飞。",
        "sign_from": "文殊菩萨第五十签"
    },
    {
        "sign_level": "上签",
        "sign_text": "好将夜月产移秋，袖里明珠莫暗投，自有贵人多着力，不须疑虑两相头。",
        "sign_from": "文殊菩萨第五十一签"
    },
    {
        "sign_level": "下签",
        "sign_text": "遭人弹驳尔官非，穷鬼揶俞灾厄时，不特公庭多险难，私家门户也防虞。",
        "sign_from": "文殊菩萨第五十二签"
    },
    {
        "sign_level": "上签",
        "sign_text": "卦中吉兆号青龙，作事求谋处处通，女嫁男婚官事吉，更无忧虑喜重重。",
        "sign_from": "文殊菩萨第五十三签"
    },
    {
        "sign_level": "下签",
        "sign_text": "横逆相交至构争，被人非是实难分，贵眸未昔悬明镜，目下心思且乱麻。",
        "sign_from": "文殊菩萨第五十四签"
    },
    {
        "sign_level": "中签",
        "sign_text": "假权恃势为良图，半是真兮半是虚，大抵临机方应变，何须缘木去求鱼。",
        "sign_from": "文殊菩萨第五十五签"
    },
    {
        "sign_level": "中签",
        "sign_text": "福来相倚好追求，命里无时也罢休，宠辱荣枯皆分定，何须险处用机谋。",
        "sign_from": "文殊菩萨第五十六签"
    },
    {
        "sign_level": "上签",
        "sign_text": "夫耕妇织利田蚕，娶得佳媳又生男，占讼占财皆得济，贵人携手任东南。",
        "sign_from": "文殊菩萨第五十七签"
    },
    {
        "sign_level": "中签",
        "sign_text": "财非天与鬼能输，散得多么积有馀，甘酒嗜音皆不顺，年来下诏果何如。",
        "sign_from": "文殊菩萨第五十八签"
    },
    {
        "sign_level": "中签",
        "sign_text": "春忙不放马蹄闲，过尽风波力险艰，直待一声鸡报晓，自然龙带宝珠还。",
        "sign_from": "文殊菩萨第六十签"
    },
    {
        "sign_level": "下签",
        "sign_text": "鸣琴断弦悄无音，万恨人间欲怨深，惆怅此园花正艳，凝霜雪积自消沉。",
        "sign_from": "文殊菩萨第六十一签"
    },
    {
        "sign_level": "上签",
        "sign_text": "女嫁男婚正及时，春蚕秋稼两相宜，市朝耸出皆荣贵，病染花颜也得医。",
        "sign_from": "文殊菩萨第六十二签"
    },
    {
        "sign_level": "上签",
        "sign_text": "几年姿色负牵丝，得婿东床正及时，正是熊罴时吉梦，孕生不觉是男儿。",
        "sign_from": "文殊菩萨第六十三签"
    },
    {
        "sign_level": "下签",
        "sign_text": "鱼游澄川愁张网，鸟入深林怕设罗，何日此身多惆碍，月愁一梦到南柯。",
        "sign_from": "文殊菩萨第六十四签"
    },
    {
        "sign_level": "中签",
        "sign_text": "守分随缘到处安，枉谋妄想必招愆，春兰秋桂随时发，处顺安常是福田。",
        "sign_from": "文殊菩萨第六十五签"
    },
    {
        "sign_level": "中签",
        "sign_text": "十年尘土困英才，吉兆依稀入梦来，好玩钱塘江上月，潮头到处海门开。",
        "sign_from": "文殊菩萨第六十六签"
    },
    {
        "sign_level": "中签",
        "sign_text": "过尽黄鸡白大乡，几多疑虑变吉祥，今朝得到江南地，开尽浮云见太阳。",
        "sign_from": "文殊菩萨第六十七签"
    },
    {
        "sign_level": "下签",
        "sign_text": "春到园苑是追游，一曲笙歌一曲愁，好事转头成恶事，提携二美到牵牛。",
        "sign_from": "文殊菩萨第六十八签"
    },
    {
        "sign_level": "下签",
        "sign_text": "欢娱变作祸胎怀，家道萧条百不谐，门里添心又未下，犬生三角又还来。",
        "sign_from": "文殊菩萨第七十签"
    },
    {
        "sign_level": "下签",
        "sign_text": "脚踏黄槐勇士奇，才高争奈赏音稀，沙场一战无消息，憔悴西风鹢退飞。",
        "sign_from": "文殊菩萨第七十一签"
    },
    {
        "sign_level": "上签",
        "sign_text": "得失荣枯本在天，乐天知命始安然，若得此意加谋望，金帛盈箱货满船。",
        "sign_from": "文殊菩萨第七十二签"
    },
    {
        "sign_level": "上签",
        "sign_text": "射中南鸣能帛书，乃知人在汉知隅，回头未遇真消息，蜡后春前且任渠。",
        "sign_from": "文殊菩萨第七十三签"
    },
    {
        "sign_level": "上签",
        "sign_text": "化龙头角久峥嵘，脱身从此谢沧溟，图化今已乘其便，得上扶摇万里程。",
        "sign_from": "文殊菩萨第七十四签"
    },
    {
        "sign_level": "中签",
        "sign_text": "曾向滩头羡美鱼，徒劳心力枉空虚，不若结网临渊去，自有金鳞应所需。",
        "sign_from": "文殊菩萨第七十五签"
    },
    {
        "sign_level": "下签",
        "sign_text": "妖忙为荣贵驱除，忧逮浓云锁太虚，若问伊家端的事，三魂七魄进华婿。",
        "sign_from": "文殊菩萨第七十六签"
    },
    {
        "sign_level": "下签",
        "sign_text": "是非名利苦相营，身势浑如架上鹰，罢脱红尘虽有志，凭谁作个解缭人。",
        "sign_from": "文殊菩萨第七十七签"
    },
    {
        "sign_level": "下签",
        "sign_text": "仇隙乘间启祸门，两家胜败未由分，更防暗箭两相射，密着机谋免自烦。",
        "sign_from": "文殊菩萨第七十八签"
    },
    {
        "sign_level": "中签",
        "sign_text": "雷雨滂沱作解时，人逢此难却除之，所有万事皆永释，实是求财若未迟。",
        "sign_from": "文殊菩萨第八十三签"
    },
    {
        "sign_level": "中签",
        "sign_text": "天时久旱遇甘雨，空谷传声闻好音，事怕不通通则变，偶遇贵人即遂心。",
        "sign_from": "文殊菩萨第八十五签"
    },
    {
        "sign_level": "中签",
        "sign_text": "失物追偿悄无情，南搜北索苦劳心，也知人向东西去，水阔渊深何处寻。",
        "sign_from": "文殊菩萨第八十六签"
    },
    {
        "sign_level": "上签",
        "sign_text": "大田多稼庆年丰，六畜蕃生用不穷，女嫁男婚官事吉，更宜作福谢高穹。",
        "sign_from": "文殊菩萨第八十七签"
    },
    {
        "sign_level": "中签",
        "sign_text": "怀宝迷邦事未宜，夜光明月暗投机，相逢按剑君知否，可以藏珠待价时。",
        "sign_from": "文殊菩萨第八十八签"
    },
    {
        "sign_level": "上签",
        "sign_text": "随时知义更知新，举动应须遇贵人，利益名扬宜进步，自然祸去福来临。",
        "sign_from": "文殊菩萨第九十一签"
    },
    {
        "sign_level": "上签",
        "sign_text": "长江浪暖鱼争戏，空谷春回花正开，得志得时须进步，好将踪迹出尘埃。",
        "sign_from": "文殊菩萨第九十二签"
    },
    {
        "sign_level": "下签",
        "sign_text": "门锁非风日色昏，长空特地起愁云，孤堂寂寞人何在，己说勤劳不遇君。",
        "sign_from": "文殊菩萨第九十五签"
    },
    {
        "sign_level": "中签",
        "sign_text": "春秋直笔照人寒，褒贬分明事事难，游夏斯时先学圣，一毫未敢露词端。",
        "sign_from": "文殊菩萨第九十七签"
    },
    {
        "sign_level": "中签",
        "sign_text": "官符口舌不曾传，百怪生来舍荒凉，夜梦更招魔鬼盛，可宜斋戒祝神明。",
        "sign_from": "文殊菩萨第九十八签"
    },
    {
        "sign_level": "上签",
        "sign_text": "自少辛勤得一官，几年埋没簿书闲，心思梦里多颠倒，颠倒谁知得自安。",
        "sign_from": "文殊菩萨第九十九签"
    },
    {
        "sign_level": "上签",
        "sign_text": "求名求利两相宜，守株待兔未为迟，片帆若得时更便，到底还他满载归。",
        "sign_from": "文殊菩萨第一百签"
    }
]